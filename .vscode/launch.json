 {
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug ts-node-dev",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node-dev",
      "runtimeArgs": ["--respawn", "--inspect=9229", "src/index.ts","src/routes.ts"],
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}

