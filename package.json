{
  "name": "simple-react-command-server",
  "version": "1.0.0",
  "description": "a simple project that shows basic of a command server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "not ready for that :Dnode dist/index.ts",
    "dev": "nodemon --watch src --exec myworkmaterial/watch_transpile_relaunchwebserv.sh",
    "dev2steps": "node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "concatproject": "./myworkmaterial/concatenate_files.sh ./package.json jest.config.ts ./src/*.{ts,tsx,json} ./*.{js,ts,tsx} tsconfig.json"
  },
  "keywords": [],
  "author": "jo",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.0.1",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
